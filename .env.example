# OmniMind Super-Intelligent AI Backend Environment Configuration

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4
OPENAI_EMBEDDING_MODEL=text-embedding-ada-002

# Optional: Whisper for Speech Recognition
WHISPER_API_KEY=your_whisper_api_key

# Optional: Web3/Blockchain Configuration
POLYGON_RPC_URL=https://polygon-rpc.com
WALLET_PRIVATE_KEY=your_wallet_private_key
CONTRACT_ADDRESS=your_smart_contract_address

# Optional: VR/AR Integration
META_APP_ID=your_meta_app_id
APPLE_VISION_PRO_API_KEY=your_apple_vision_pro_api_key

# Application Configuration
NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:3000

# Database Configuration (if using direct PostgreSQL)
DATABASE_URL=postgresql://username:password@localhost:5432/omnimind

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Analytics Configuration
GOOGLE_ANALYTICS_ID=your_ga_id
MIXPANEL_TOKEN=your_mixpanel_token

# Security Configuration
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Feature Flags
ENABLE_VR_FEATURES=true
ENABLE_WEB3_FEATURES=true
ENABLE_SPEECH_RECOGNITION=true
ENABLE_REAL_TIME_ANALYTICS=true
